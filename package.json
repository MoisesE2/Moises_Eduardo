{
  "name": "moises-eduardo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run server\"",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:a11y": "jest --testPathPattern=a11y",
    "test:performance": "jest --testPathPattern=performance",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "serve": "serve -s dist -p 4173",
    "server": "json-server --watch src/api/db.json --port 3001",
    "sitemap": "node scripts/generate-sitemap.js",
    "i18n:extract": "i18next-scanner",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "prepare": "npm run type-check && npm run lint:check"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "i18next": "^25.3.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.1.5",
    "zod": "^3.25.73"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.19.0",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-scroll": "^1.8.10",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^30.0.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jest-transform-stub": "^2.0.0",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.5.2",
    "serve": "^14.2.4",
    "tailwindcss": "^4.0.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.1.0"
  }
}
